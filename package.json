{
    "name": "dealership-miner",
    "version": "1.0.0",
    "author": "Jordon de Hoog @jordond",
    "repository": "jordond/dealership-miner",
    "homepage": "https://github.com/jordond/dealership-miner",
    "bugs": "https://github.com/jordond/dealership-miner/issues",
    "license": "MIT",
    "private": true,
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "bin": {
        "dealership-miner": "./bin/run"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "engines": {
        "node": ">=10.0.0"
    },
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "prebundle": "rm -rf dist",
        "bundle": "npm run bundle:pack && npm run bundle:win",
        "bundle:pack": "oclif-dev pack",
        "bundle:win": "oclif-dev pack:win",
        "publish": "npm run publish:pack && npm run publish:win",
        "publish:pack": "oclif-dev publish",
        "publish:win": "oclif-dev publish:win",
        "test": "echo NO TESTS",
        "version": "oclif-dev readme && git add README.md"
    },
    "dependencies": {
        "@googlemaps/google-maps-services-js": "^3.1.6",
        "@oclif/command": "^1",
        "@oclif/config": "^1",
        "@oclif/plugin-commands": "^1.3.0",
        "@oclif/plugin-help": "^3",
        "@oclif/plugin-not-found": "^1.2.4",
        "@oclif/plugin-update": "^1.3.10",
        "@oclif/plugin-warn-if-update-available": "^1.7.0",
        "cli-ux": "^5.5.0",
        "fs-extra": "^9.0.1",
        "inquirer": "^7.3.3",
        "listr": "^0.14.3",
        "mkdirp": "^1.0.4",
        "pull-file": "^1.1.0",
        "pull-stream": "^3.6.14",
        "pull-tobits": "^0.2.1",
        "tslib": "^1",
        "unzipper": "^0.10.11"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1",
        "@types/fs-extra": "^9.0.1",
        "@types/inquirer": "^7.3.1",
        "@types/listr": "^0.14.2",
        "@types/mkdirp": "^1.0.1",
        "@types/node": "^10",
        "@types/pull-stream": "^3.6.2",
        "@types/unzipper": "^0.10.3",
        "aws-sdk": "^2.753.0",
        "eslint": "^5.13",
        "eslint-config-oclif": "^3.1",
        "eslint-config-oclif-typescript": "^0.1",
        "eslint-config-prettier": "^6.11.0",
        "globby": "^10",
        "makensis": "^0.21.2",
        "pkg": "^4.4.9",
        "ts-node": "^8",
        "typescript": "^3.3"
    },
    "oclif": {
        "commands": "./lib/commands",
        "bin": "dealership-miner",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-not-found",
            "@oclif/plugin-commands",
            "@oclif/plugin-update",
            "@oclif/plugin-warn-if-update-available"
        ],
        "update": {
            "s3": {
                "bucket": "dealership-miner"
            }
        }
    },
    "pkg": {
        "scripts": "build/**/*.js",
        "assets": "build/**/*.json"
    }
}
